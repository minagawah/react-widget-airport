{
  "name": "react-widget-airport",
  "version": "0.1.4",
  "private": false,
  "description": "A React widget for Airport animations with flight departures/arrivials.",
  "main": "dist/airport.js",
  "scripts": {
    "pretty": "$npm_execpath prettier -w \"**/*.(js|jsx)\"",
    "build:dev": "$npm_execpath webpack --config webpack.dev.js",
    "build": "$npm_execpath webpack --config webpack.prod.js",
    "serve": "http-server dist -a localhost -p 8080"
  },
  "repository": "https://github.com/minagawah/react-widget-airport.git",
  "keywords": [
    "react",
    "widget",
    "pixi",
    "react-pixi-fiber"
  ],
  "author": "Hiroki Minagawa <minagawa@tokyo800.jp>",
  "license": "(Unlicense OR MIT)",
  "bugs": {
    "url": "https://github.com/minagawah/react-widget-airport/issues"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "pixi.js": "^5.3.3",
    "pixi.js-legacy": "^5.3.3",
    "ramda": "^0.27.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-pixi-fiber": "1.0.0-beta.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/runtime-corejs3": "^7.12.5",
    "autoprefixer": "^10.0.4",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "3",
    "css-loader": "^5.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "http-server": "^0.12.3",
    "license-webpack-plugin": "^2.3.1",
    "postcss-loader": "^4.1.0",
    "prettier": "^2.2.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0",
    "webpack-merge": "^5.4.0"
  }
}
